

object SetExample {
  
  val myset1: Set[Int] = Set(1,2,5,8,9,6,4)
  val myset2: Set[Int] = Set(4,2,9,18,19,16,14)
  val names: Set[String] = Set("Max", "Tom", "John")
  
  def main(args: Array[String]){
    
    println(myset1 + 10)
    println(myset1)
    println(names("Max"))
    
    println(myset1.head)
    println(myset1.tail)
    println(myset1.isEmpty)
    
    println(myset1 ++ myset2)
    println(myset1.++(myset2))
    
    println(myset1.&(myset2))
    println(myset1 && myset2)
    
  }
}